{
  "npmScope": "automapper",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "core": {
      "tags": ["type:library"]
    },
    "classes": {
      "tags": ["type:plugin"]
    },
    "types": {
      "tags": ["type:type-defs"]
    },
    "integration-test": {
      "tags": ["type:test"]
    },
    "pojos": {
      "tags": ["type:plugin"]
    },
    "nestjs": {
      "tags": ["type:integration"]
    },
    "nestjs-integration-test": {
      "tags": ["type:test"]
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  }
}
